<div class="bus-station--title">
  <h3><%= name %></h3>
</div>
<div class="bus-route--container"></div>
<div class="bus-route--ticker">
<!-- Include the ticker script -->
<script src="liScroller/jquery.li-scroller.1.0.js"></script>

<!-- The Actual Ticker Data -->
<ul class="bus-route-ticker" id="ticker">
	<li>Route 26 in 23,43 mins.</li>
	<li>Route 28 in 33 mins.</li>
</ul>

<!-- The actual ticker; Travelocity controls speed -->
<script type="text/javascript">
$(function(){
	$("ul#ticker").liScroll({travelocity: 0.15});
});
</script></div>
